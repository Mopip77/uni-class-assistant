<view><van-overlay vue-id="1" show="true" bind:__l="__l" vue-slots="{{['default']}}"><view class="update-userinfo-box"><view class="avatar"><view class="box"><van-image vue-id="{{('2')+','+('1')}}" round width="100" height="100" lazy-load src="{{loginUser.avatarUrl}}" bind:__l="__l"></van-image></view></view><view class="name-field"><text>真实姓名</text><van-cell-group vue-id="{{('3')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><input type="text" placeholder="请输入真实姓名" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></van-cell-group></view><view class="school-field"><text>学生信息</text><van-cell-group vue-id="{{('4')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><input type="text" placeholder="请输入学校名" data-event-opts="{{[['input',[['__set_model',['','schoolName','$event',[]]]]]]}}" value="{{schoolName}}" bindinput="__e"/><input type="text" placeholder="请输入教职工号" data-event-opts="{{[['input',[['__set_model',['','stuNumber','$event',[]]]]]]}}" value="{{stuNumber}}" bindinput="__e"/></van-cell-group></view><view class="button-group"><van-button vue-id="{{('5')+','+('1')}}" type="info" custom-style="height: 70rpx;width: 140rpx;" data-event-opts="{{[['^tap',[['cancel']]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}">取消</van-button><van-button vue-id="{{('6')+','+('1')}}" type="primary" custom-style="height: 70rpx;width: 140rpx;" data-event-opts="{{[['^tap',[['submit']]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}">提交</van-button></view></view></van-overlay></view>